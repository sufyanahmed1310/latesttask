pipeline {
    agent any

    stages {
        stage('java') {
            steps {
                git branch: 'java', url: 'https://github.com/sufyanahmed1310/latesttask.git'
                bat 'javac HelloWorld.java'
                bat 'java HelloWorld'
            }
        }
        stage('shell') {
            steps {
                git branch: 'shell', url: 'https://github.com/sufyanahmed1310/latesttask.git'
            }
        }
        stage('shell script') {
            steps {
                sh './script.sh'
            }
        }
        stage('maven') {
            steps {
                git branch: 'feature', url: 'https://github.com/sufyanahmed1310/latesttask.git'
            }
        }
        stage('install maven') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
}

